{
  "name": "se_school",
  "version": "1.0.0",
  "type": "module",
  "description": "Software Engineering School 4.0 // Case",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run migration:run && NODE_ENV=development tsx src/index.ts",
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "build": "tsc",
    "prod": "NODE_ENV=production node ./dist/index.js",
    "env:dev": "docker-compose --project-directory ./ -f ./environments/dev.yaml --env-file ./.env.development up",
    "env:prod": "docker-compose --project-directory ./ --env-file ./.env.production up",
    "prepare": "husky install",
    "lint": "eslint ./src",
    "format": "prettier .",
    "format:lint": "npm run lint && npm run format",
    "typeorm": "typeorm-ts-node-esm",
    "migration:create": "npm run typeorm migration:create",
    "migration:run": "npm run typeorm migration:run -- -d ./src/orm/data-source.ts",
    "migration:prod:run": "npm run typeorm migration:run -- -d ./dist/orm/data-source.js",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/orm/data-source.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^3.1.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^53.0.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.10.3"
  }
}